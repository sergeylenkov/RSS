  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint --config ./config/eslint.config.js --ext .ts,.js,.tsx src/",
    "lint:report": "eslint --config ./config/eslint.config.js --ext .ts,.js,.tsx --format json --output-file reports/eslint-report.json src/",
    "lint:styles": "stylelint --config ./config/stylelint.config.js **/*.scss",
    "test": "jest --config ./config/jest.config.js --coverage=false",
    "test:coverage": "jest --config ./config/jest.config.js --coverage=true",
    "test:update": "jest --config ./config/jest.config.js --update-snapshot",
    "scan": "npm run test:coverage && npm run lint:report",
    "prettier:check": "prettier --config ./config/prettier.config.js --check .",
    "prettier:write": "prettier --config ./config/prettier.config.js --write ."
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
